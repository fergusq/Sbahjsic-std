target("1.1.0")

setHelp(putstr, "Prints its argument as a string.")
native putstr

setHelp(putln, "Prints its argument and a newline.")
function putln(arg)
	putstr(_string(arg) + "\n")
endfunction

setHelp(input, "Returns line of user input or false if an error occurs.")
native input

setHelp(real_input, "The real input function. Returns the next line of user input or undefined if an error occurs.")
native real_input
